{
  "name": "setup-imagemagick",
  "version": "2.0.0",
  "description": "GitHub action to setup imagemagick",
  "main": "lib/index.js",
  "scripts": {
    "all": "npm run lint && npm run build && npm run package",
    "build": "tsc",
    "fix": "prettier --write **/*.{ts,json,md}",
    "lint": "prettier --check **/*.{ts,json,md}",
    "package": "ncc build --license licenses.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mfinelli/setup-imagemagick.git"
  },
  "keywords": [
    "imagemagick"
  ],
  "author": "Mario Finelli",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mfinelli/setup-imagemagick/issues"
  },
  "homepage": "https://github.com/mfinelli/setup-imagemagick#readme",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.1.3",
    "@vercel/ncc": "^0.36.1",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
